{
  "manifest_version": 2,

  "name": "Skip for YouTube Playlists",
  "description": "Skip to the next song on your playlists from anywhere in Chrome.",
  "version": "0.0.0.1",
  "permissions": [
  "tabs",
	"activeTab",
	"http://*/*",
	"https://*/*"  ],
  "content_scripts" : [
  {
    "matches": ["*://www.facebook.com/*", "*://www.facebook.com/"],
    "js": ["util/jquery.min.js", "js/fbedit.js"],
    "run_at": "document_end"
  }
  ],
  "commands": {
          "toggle-feature-foo": {
            "suggested_key": {
              "default": "Ctrl+Shift+5"
            },
            "description": "Toggle feature foo",
            "global": true
          }
        }
}